{
  "redirects": [
    { "source": "/about-us", "destination": "/about", "permanent": true },
    { "source": "/about-new", "destination": "/about", "permanent": true },
    { "source": "/bookkeeping-services", "destination": "/services/bookkeeping", "permanent": true },
    { "source": "/technology-consulting", "destination": "/services/technology-consulting", "permanent": true },
    { "source": "/electrical-consulting", "destination": "/services/consulting", "permanent": true },
    { "source": "/hvac-consulting", "destination": "/services/consulting", "permanent": true },
    { "source": "/services-old", "destination": "/services", "permanent": true },
    { "source": "/contact-simple", "destination": "/contact", "permanent": true },
    { "source": "/contact-backup", "destination": "/contact", "permanent": true },
    { "source": "/contact-debug", "destination": "/contact", "permanent": true },
    { "source": "/contact-test", "destination": "/contact", "permanent": true },
    { "source": "/contact-minimal", "destination": "/contact", "permanent": true },
    { "source": "/contact-new", "destination": "/contact", "permanent": true },
    { "source": "/leadership", "destination": "/team", "permanent": true },
    { "source": "/austin", "destination": "/locations/texas/austin", "permanent": true },
    { "source": "/dallas", "destination": "/locations/texas/dallas", "permanent": true },
    { "source": "/houston", "destination": "/locations/texas/houston", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=3600, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}
